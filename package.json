{
  "name": "ens-app-v2",
  "version": "0.1.108",
  "workspaces": [
    "test-environment"
  ],
  "scripts": {
    "dev": "NODE_NO_WARNINGS=1 next dev",
    "dev:glocal": "NEXT_PUBLIC_GRAPH_URI=http://localhost:8000/subgraphs/name/graphprotocol/ens next dev",
    "build": "next build",
    "build:glocal": "NEXT_PUBLIC_GRAPH_URI=http://localhost:8000/subgraphs/name/graphprotocol/ens next build",
    "start": "next start",
    "buildandstart:glocal": "yarn build:glocal && yarn start",
    "env": "yarn workspace test-environment",
    "env:load": "yarn env load",
    "env:start": "yarn env start",
    "env:start:ci": "yarn env start:ci",
    "env:start:root": "yarn env start:root",
    "lint": "next lint",
    "export": "next export",
    "analyze": "ANALYZE=true yarn build",
    "analyse": "yarn analyze",
    "test": "jest --env=./custom-test-env.js",
    "test:watch": "jest --env=./custom-test-env.js --watch",
    "test:coverage": "jest --env=./custom-test-env.js --coverage",
    "waiton:all": "wait-on http://localhost:3000 && wait-on http://localhost:8040",
    "cypress:start": "CYPRESS_REMOTE_DEBUGGING_PORT=9222 cypress run --headed --browser chrome",
    "cypress:ci": "yarn cypress:start",
    "cypress:wait": "yarn waiton:all && yarn cypress:start",
    "cypress:wait:ci": "yarn waiton:all && yarn cypress:ci",
    "e2e": "yarn node ./scripts/e2e.js",
    "e2e:root": "yarn node ./scripts/e2e.js root",
    "e2e:ci": "yarn node ./scripts/e2e.js ci",
    "postinstall": "husky install",
    "postyalc": "yarn"
  },
